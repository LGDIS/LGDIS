# 災害訓練モードプロジェクトID
training_prj:
  2: true

# 通信試験モードプロジェクトID
test_prj:
  3: true

# issue_patch.rb 配信メッセージ作成処理メソッド群
# 外部配信先が追加になった場合追加必要
create_msg_method:
  - &create_msg_method_1 create_commons_msg
  - &create_msg_method_2 create_smtp_msg
  - &create_msg_method_3 create_twitter_msg
  - &create_msg_method_4 create_facebook_msg
  - &create_msg_method_5 create_atom_msg

# plugins/lgdis/app/workers/ext_out 以下worker 群
# 外部配信先が追加になった場合追加必要
delivery_job_class:
  - &delivery_job_class_1 "ExtOut::CommonsRequestJob"
  - &delivery_job_class_2 "ExtOut::SmtpRequestJob"
  - &delivery_job_class_3 "ExtOut::TwitterRequestJob"
  - &delivery_job_class_4 "ExtOut::FacebookRequestJob"
  - &delivery_job_class_5 "ExtOut::AtomRequestJob"

to: # 配送先メールアドレス、メーリングリスト
  - &to_err apl@localhost.localdomain # Resqueジョブエラー通知
  - &to_1   to1@localhost.localdomain # 自治体職員向けメール（０号配備）
  - &to_2   to2@localhost.localdomain # 自治体職員向けメール（１号配備）
  - &to_3   to3@localhost.localdomain # 自治体職員向けメール（２号配備）
  - &to_4   to4@localhost.localdomain # 自治体職員向けメール（３号配備）
  - &to_5   to5@localhost.localdomain # まちcomi向けメール

from: # 送信元メールアドレス、メーリングリスト
  - &from_err frome@localhost.localdomain # Resqueジョブエラー通知
  - &from_1   from1@localhost.localdomain # 自治体職員向けメール
  - &from_2   from2@localhost.localdomain # まちcomi向けメール

smtp_server: # SMTPサーバー
  - &smtp_server_err  # Resqueジョブエラー通知
    mta_fqdn: localhost.localdomain
    port:     25
    domain:   localdomain
    charset:  utf-8
  - &smtp_server_1 # 自治体職員向けメール
    mta_fqdn: localhost.localdomain
    port:     25
    domain:   localdomain
    charset:  utf-8
  - &smtp_server_2 # まちcomi向けメール
    mta_fqdn:   localhost.localdomain
    port:       25
    domain:     localdomain
    user_name:  apl
    password:   JBC03142
    charset:    utf-8

# 外部配信先設定値
# 外部配信先が追加になった場合追加必要
delivery_place:
  &id 1: &commons
    id: *id
    name: 公共情報コモンズ
    create_msg_method:  *create_msg_method_1
    delivery_job_class: *delivery_job_class_1
  &id 2: &smtp_0
    id: *id
    name: 自治体職員向けメール（０号配備）
    create_msg_method:  *create_msg_method_2
    delivery_job_class: *delivery_job_class_2
    to:                 *to_1
    from:               *from_1
    smtp_server:        *smtp_server_1
  &id 3: &smtp_1
    id: *id
    name: 自治体職員向けメール（１号配備）
    create_msg_method:  *create_msg_method_2
    delivery_job_class: *delivery_job_class_2
    to:                 *to_2
    from:               *from_1
    smtp_server:        *smtp_server_1
  &id 4: &smtp_2
    id: *id
    name: 自治体職員向けメール（２号配備）
    create_msg_method:  *create_msg_method_2
    delivery_job_class: *delivery_job_class_2
    to:                 *to_3
    from:               *from_1
    smtp_server:        *smtp_server_1
  &id 5: &smtp_3
    id: *id
    name: 自治体職員向けメール（３号配備）
    create_msg_method:  *create_msg_method_2
    delivery_job_class: *delivery_job_class_2
    to:                 *to_4
    from:               *from_1
    smtp_server:        *smtp_server_1
  &id 6: &smtp_auth
    id: *id
    name: メーリングリスト
    create_msg_method:  *create_msg_method_2
    delivery_job_class: *delivery_job_class_2
    to:                 *to_5
    from:               *from_2
    smtp_server:        *smtp_server_2
  &id 7: &twitter
    id: *id
    name: Twitter
    create_msg_method:  *create_msg_method_3
    delivery_job_class: *delivery_job_class_3
  &id 8: &facebook
    id: *id
    name: Facebook
    create_msg_method:  *create_msg_method_4
    delivery_job_class: *delivery_job_class_4
  &id 9: &atom
    id: *id
    name: XML/HTML/RSS
    create_msg_method:  *create_msg_method_5
    delivery_job_class: *delivery_job_class_5
  &id 10: &urgent_mail_docomo
    id: *id
    name: 緊急速報メール（NTTDocomo）
    create_msg_method:  *create_msg_method_1
    delivery_job_class: *delivery_job_class_1
    commons_xml_carrier: UMC1      # 受信媒体(キャリア)
  &id 11: &urgent_mail_au
    id: *id
    name: 緊急速報メール（KDDI）
    create_msg_method:  *create_msg_method_1
    delivery_job_class: *delivery_job_class_1
    commons_xml_carrier: UMC2      # 受信媒体(キャリア)
  &id 12: &urgent_mail_sb
    id: *id
    name: 緊急速報メール（ソフトバンク）
    create_msg_method:  *create_msg_method_1
    delivery_job_class: *delivery_job_class_1
    commons_xml_carrier: UMC3      # 受信媒体(キャリア)

