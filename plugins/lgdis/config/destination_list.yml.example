  comp_info: 5        # 補足情報

# 公共情報コモンズへの発信
# NOTE:
# endpoint below have to point to a URL
# where COMMONS web server process is running on.
# In other word, IP address of non-router (including virtualizer router).
# ie commons_endpoint: http://XXX.XXX.XXX.XXX/commons/subscriber/soap/testservice.example.com/
# sha
commons_connection:
  endpoint: https://lgdis.local/commons/subscriber/soap/testservice.example.com/
  #sha centos_ssh: commons_endpoint: https://localhost:443/commons/subscriber/soap/testservice.example.com/
  username: user1@example.com
  password: password
  wsdl: /home/rals/LGDIS/plugins/lgdis/lib/lgdis/ext_out/wsdl/MQService.wsdl
  namespace: http://soap.publiccommons.ne.jp/
  ssl_ca_cert: /opt/fix/SOAPdev/etc/pki/CA/cacert.pem

# 災害情報ポータル連携
# Atom出力先設定
atom:
  # 出力ディレクトリ名
  output_dir: ./public/plugin_assets/lgdis/atomfeeds
  # 出力ファイル名
  # ※文字列中の「${trackerid}」は該当のトラッカーIDに、「${date}」は作成年月日時分秒(YYYYMMDDHHMMSS)に置換されます
  output_filename: "tracker_${trackerid}.rss"

# 災害情報ポータルURL
disaster_portal_url: "下記リンクより、災害情報を確認して下さい。\n http://www.jice.or.jp/sinsai/sinsai_result.php"

# *号配備メールに添付するLGDSF URL 設定
lgdsf_url: "下記リンクより、位置情報、参集場所の報告を行ってください。\n http://XXX.XXX.XXX.XXX/staffs/send/"

# 外部出力
ext_out:
  archives_dir: ./log/ext_out  # アーカイブ出力先ディレクトリ
