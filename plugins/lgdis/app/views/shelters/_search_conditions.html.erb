<%= f.hidden_field :project_id_eq, :value => project.id %>
<div class="splitcontent">
  <div class="splitcontentleft">
    <%= field_for :shelter_type do |key, label| %>
      <%= label_tag{label} %>
      <%= f.select("#{key}_eq", options_for_select(@shelter_const[key.to_s].invert),:include_blank => true) %>
    <% end %>
    
    <%= field_for :location_area, :label => "地区名" do |key, label| %>
      <%= label_tag{label} %>
      <%= select_tag key,'' %>
    <% end %>
    
    <%= field_for :usable_flag do |key, label| %>
      <%= label_tag{label} %>
      <%= f.select("#{key}_eq", options_for_select(@shelter_const[key.to_s].invert),:include_blank => true) %>
    <% end %>
    
    <%= field_for :opened_at do |key, label| %>
      <%= label_tag{label} %>
      <%= f.text_field "#{key}_gte", :size=>'10' %><%= calendar_for("search_#{key}_gte") %>
      ～
      <%= f.text_field "#{key}_lte", :size=>'10' %><%= calendar_for("search_#{key}_lte") %>
    <% end %>
  </div>
  
  <div class="splitcontentright">
    <%= field_for :name do |key, label| %>
      <%= label_tag{label} %>
      <%= select_tag key,'' %>
    <% end %>
    
    <%= field_for :openable_flag do |key, label| %>
      <%= label_tag{label} %>
      <%= f.select("#{key}_eq", options_for_select(@shelter_const[key.to_s].invert),:include_blank => true) %>
    <% end %>
    
    <%= field_for :closed_at do |key, label| %>
      <%= label_tag{label} %>
      <%= f.text_field "#{key}_gte", :size=>'10' %><%= calendar_for("search_#{key}_gte") %>
      ～
      <%= f.text_field "#{key}_lte", :size=>'10' %><%= calendar_for("search_#{key}_lte") %>
    <% end %>
    <%= field_for :status do |key, label| %>
      <%= label_tag{label} %>
      <%= f.select("#{key}_eq", options_for_select(@shelter_const[key.to_s].invert),:include_blank => true) %>
    <% end %>
  </div>
</div>

