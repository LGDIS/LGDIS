<div class="splitcontent">
  <div class="splitcontentleft">
    <%= field_for :current_sort_criteria do |key, label| %>
      <%= label_tag{label} %>
      <%= f.radio_button "#{key}_eq", "", :checked => true %>なし
      <% @evacuation_advisory_const["current_sort_criteria"].each do |sc_key, sc_value| %>
        <%= f.radio_button "#{key}_eq", sc_key %><%= sc_value %>
      <% end %>
    <% end %>
    
    <%= field_for :area do |key, label| %>
      <%= label_tag{label} %>
      <%= f.text_field "#{key}_like", :value => @search.area_like, :size=>'50' %>
    <% end %>
  </div>
  
  <div class="splitcontentright">
  </div>
</div>

<% include_calendar_headers_tags %>
