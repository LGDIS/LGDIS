<% if issue.new_record? %>
<%= javascript_tag do %>
  $(function(){
    <% issue.editable_custom_field_values.each do |cfv|
        cf = cfv.custom_field
        next unless cf.is_default_all_selected == true
        next unless cfv.value[0].nil? %>
    $("#<%= "issue_custom_field_values_#{cf.id}" %> option").prop("selected",true);
    <% end -%>
  });
<% end %>
<% end %>